[application]

config/name="L4DRL 7DRL"
config/description="A 7 Day Roguelike(leaning on roguelite) demake of Left 4 Dead. Sort of inspired by DOOMRL. Since this is a seven day roguelike this aims to recreate some of L4D's gameplay with some roguelike elements.

Mostly based of L4D2's gameplay and not L4D1."
run/main_scene="res://main_scenes/MainMenuUI.tscn"
config/icon="res://icon.png"

[autoload]

Enumerations="*res://main_scenes/Enumerations.gd"
Utilities="*res://main_scenes/Utilities.gd"
GamePreferences="*res://main_scenes/GamePreferences.gd"
Globals="*res://main_scenes/Globals.gd"
AudioGlobal="*res://main_scenes/AudioGlobal.gd"

[debug]

gdscript/warnings/unassigned_variable=false
gdscript/warnings/unassigned_variable_op_assign=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/unreachable_code=false
gdscript/warnings/variable_conflicts_function=false
gdscript/warnings/function_conflicts_variable=false
gdscript/warnings/function_conflicts_constant=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

